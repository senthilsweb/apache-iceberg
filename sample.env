# Catalog Configuration
ICE_CATALOG_TYPE=rest  # Options: rest (Nessie), sql (SQLite)

# Nessie REST Catalog
ICE_NESSIE_URI=http://localhost:19120/iceberg/
ICE_WAREHOUSE_PATH=s3://your-bucket/warehouse

# SQLite Catalog (for local development)
ICE_SQL_CATALOG_URI=sqlite:///./warehouse/catalog.db
ICE_SQL_WAREHOUSE_PATH=./warehouse

# S3-Compatible Storage
R2_ENDPOINT=https://your-account.r2.cloudflarestorage.com
R2_ACCESS_KEY=your-access-key
R2_SECRET_KEY=your-secret-key

# Data Loading
ICE_CSV_FILE_PATH=/path/to/your/data.csv
ICE_TABLE_NAME=your_table_name
ICE_NAMESPACE=default
